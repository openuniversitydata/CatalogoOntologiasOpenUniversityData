    <!-- http://w3id.org/devops-infra/server#assignmentStatus -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/server#assignmentStatus">
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/server#Server"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="http://w3id.org/devops-infra/kos/assignment-status"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The status related to the assignment of a Server, which can be in use, ready, apply recovery, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">assignment Status</rdfs:label>
    </owl:ObjectProperty>
    


