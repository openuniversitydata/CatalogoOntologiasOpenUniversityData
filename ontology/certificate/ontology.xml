    <!-- http://w3id.org/devops-infra/certificate#certApplyPerson -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#certApplyPerson">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The person in charge of the certificate application</rdfs:comment>
        <rdfs:label xml:lang="en">certificate application person</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#certDeployPerson -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#certDeployPerson">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The person in charge of the certificate deployment</rdfs:comment>
        <rdfs:label xml:lang="en">certificate deployment person</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#certDeployRegion -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#certDeployRegion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The region for the certificate deployment</rdfs:comment>
        <rdfs:label xml:lang="en">certificate deployment region</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#certPurchaseNumber -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#certPurchaseNumber">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The purchase number of the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">certificate purchase number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#certType -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#certType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The type of certificate</rdfs:comment>
        <rdfs:label xml:lang="en">certificate type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#certificateContent -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#certificateContent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The content of the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">certificate content</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#domain -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#domain">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Also referred to as common name (CN). It is the fully qualified domain name for the certificate, the deployment of the certificate, or the certificate signing request. For example, *.example.org</rdfs:comment>
        <rdfs:label xml:lang="en">domain</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#expiryDate -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#expiryDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">The expiry date of the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">expiry date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#issuer -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#issuer">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The issuer of the certificate, also known as issuer common name</rdfs:comment>
        <rdfs:label xml:lang="en">issuer</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#issuerOrganisation -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#issuerOrganisation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The organisation that issues the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">issuer organisation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#issuerOrganisationUnit -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#issuerOrganisationUnit">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The organisational unit that issues the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">issuer organisation unit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#privateKey -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#privateKey">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The private key of the certificate or the certificate signing request</rdfs:comment>
        <rdfs:label xml:lang="en">private key</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#publicKey -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#publicKey">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#CertificateSigningRequest"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The public key of the certificate signing request</rdfs:comment>
        <rdfs:label xml:lang="en">public key</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#realSerialNumber -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#realSerialNumber">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The real serial number of the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">real serial number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#region -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#region">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The region for the certificate, the deployment of the certificate, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">region</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#serialNumber -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#serialNumber">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The serial number of the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">serial number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#service -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#service">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificateDeployment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The service for which the Digital Certificate is deployed</rdfs:comment>
        <rdfs:label xml:lang="en">service</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#site -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#site">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificateDeployment"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The site for which the Digital Certificate is deployed</rdfs:comment>
        <rdfs:label xml:lang="en">site</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#startDate -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#startDate">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">The start date of the certificate</rdfs:comment>
        <rdfs:label xml:lang="en">start date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#user -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#user">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The user of the certificate or certificate signing request, also known as user common name</rdfs:comment>
        <rdfs:label xml:lang="en">user</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#userOrganisation -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#userOrganisation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Also known as Organization Name (O). It is usually the legal name of a company or entity that is using the certificate, or has made the certificate signing request, and should commonly include any suffixes such as Ltd., Inc., or Corp. For example, Example Organisation, Ltd</rdfs:comment>
        <rdfs:label xml:lang="en">user organisation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#userOrganisationCountry -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#userOrganisationCountry">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Also referred to as country (C). The two-letter ISO code for the country of the organisation that uses the Digital Certificate or has made the Certificate Signing Request. For instance, ES</rdfs:comment>
        <rdfs:label xml:lang="en">user organisation country</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#userOrganisationEmail -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#userOrganisationEmail">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Also referred to as email address (EMAIL). The email contact (usually the certificate administrator or IT department) for the country of the organisation that uses the Digital Certificate or has made the Certificate Signing Request. For instance, info@example.org</rdfs:comment>
        <rdfs:label xml:lang="en">user organisation email</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#userOrganisationLocality -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#userOrganisationLocality">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Also referred to as locality (L). The name of the locality (town, city, village, etc.) of the organisation that uses the Digital Certificate or has made the Certificate Signing Request. For instance, Boadilla del Monte</rdfs:comment>
        <rdfs:label xml:lang="en">user organisation locality</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#userOrganisationState -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#userOrganisationState">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Also referred to as state (S). The name of the locality (province, region, county or state) of the organisation that uses the Digital Certificate or has made the Certificate Signing Requestte or Certificate Signing Request. For instance, Comunidad de Madrid</rdfs:comment>
        <rdfs:label xml:lang="en">user organisation state</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#userOrganisationUnit -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#userOrganisationUnit">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Also known as Organizational Unit (OU). It is the organisational unit of the user of the certificate or certificate signing request. It is an internal  department/division name. For example, IT Support</rdfs:comment>
        <rdfs:label xml:lang="en">user organisation unit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/certificate#valid -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/certificate#valid">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/certificate#DigitalCertificate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">It determines whether the certificate is valid</rdfs:comment>
