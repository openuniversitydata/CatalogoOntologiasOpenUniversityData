    <!-- http://w3id.org/devops-infra/product#calls -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#calls">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/devops-infra/core#dependsOn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#Service"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#ServiceInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#MicroService"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#MicroServiceInstance"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#Service"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#ServiceInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A service or a service instance may call other services or microservices, or their corresponding instances</rdfs:comment>
        <rdfs:label xml:lang="en">calls</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#clusterType -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#clusterType">
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/product#ServiceCluster"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="http://w3id.org/devops-infra/kos/cluster-type"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The type of a Service Cluster. The allowed values will be represented in a SKOS codelist. They can be, for example, production, redundancy, grey, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">cluster type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#deployedInstanceOf -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#deployedInstanceOf">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#BusinessProductInstance"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#MicroServiceInstance"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#ServiceInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#BusinessProduct"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#MicroService"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A microservice instance, a service instance or a business product instance are instances of microservices, services or business products deployed in some infrastructure</rdfs:comment>
        <rdfs:label xml:lang="en">deployed instance of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#hasProductOffering -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#hasProductOffering">
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/product#BusinessProduct"/>
        <rdfs:range rdf:resource="http://w3id.org/devops-infra/product#ProductOffering"/>
        <rdfs:label xml:lang="en">A Business Product may have a number of commercial product offerings, which are usually made available in a product catalogue</rdfs:label>
        <rdfs:label xml:lang="en">has product offering</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#hasProductVersion -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#hasProductVersion">
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/product#ProductOffering"/>
        <rdfs:range rdf:resource="http://w3id.org/devops-infra/product#ProductVersion"/>
        <rdfs:comment xml:lang="en">A Product Offering may have different Product Versions offered in the corresponding catalogue</rdfs:comment>
        <rdfs:label xml:lang="en">has product version</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#offeredBy -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#offeredBy">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/devops-infra/organisation#scope"/>
        <rdfs:range rdf:resource="http://w3id.org/devops-infra/organisation#Scope"/>
        <rdfs:comment xml:lang="en">Business products, product offerings and services are offered by a scope (and its corresponding business department)</rdfs:comment>
        <rdfs:label xml:lang="en">offered by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#runsInServiceCluster -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#runsInServiceCluster">
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/product#MicroService"/>
        <rdfs:range rdf:resource="http://w3id.org/devops-infra/product#ServiceCluster"/>
        <rdfs:comment xml:lang="en">A Microservice may run in a service cluster</rdfs:comment>
        <rdfs:label xml:lang="en">runs in service cluster</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#serviceType -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#serviceType">
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="http://w3id.org/devops-infra/kos/service-type"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The type of a service. It can be any of the following, according to the corresponding SKOS Concept Scheme: common, middleware, HWS</rdfs:comment>
        <rdfs:label xml:lang="en">service type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#uses -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#uses">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/devops-infra/core#dependsOn"/>
        <rdfs:range rdf:resource="http://w3id.org/devops-infra/product#Service"/>
        <rdfs:comment xml:lang="en">A Product Offering or a Business Product use services</rdfs:comment>
        <rdfs:label xml:lang="en">uses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#usesServiceModule -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#usesServiceModule">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#MicroService"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#Service"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/devops-infra/software#ServiceModule"/>
        <rdfs:comment xml:lang="en">The service modules that are being used by a product, a product offering, a service or a microservice</rdfs:comment>
        <rdfs:label xml:lang="en">uses service module</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/product#usesServiceModuleVersion -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/product#usesServiceModuleVersion">
        <rdfs:domain rdf:resource="http://w3id.org/devops-infra/product#ProductVersion"/>
        <rdfs:range rdf:resource="http://w3id.org/devops-infra/software#ServiceModuleVersion"/>
        <rdfs:comment xml:lang="en">A product version may use several versions of the service modules that are being used by the product</rdfs:comment>
        <rdfs:label xml:lang="en">uses service module version</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/devops-infra/server#autoScalingHostConfig -->


    <owl:ObjectProperty rdf:about="http://w3id.org/devops-infra/server#autoScalingHostConfig">
