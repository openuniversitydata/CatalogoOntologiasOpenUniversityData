        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#deployedInstanceOf"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/devops-infra/product#BusinessProduct"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#uses"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/devops-infra/product#ServiceInstance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#deployedInstanceOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
