                <owl:allValuesFrom rdf:resource="http://w3id.org/devops-infra/server#AutoScalingHostConfig"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#isDeleted"/>
