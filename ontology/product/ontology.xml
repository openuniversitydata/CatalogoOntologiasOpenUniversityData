    <!-- http://w3id.org/devops-infra/product#ip -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/product#ip">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#MicroServiceInstance"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#ServiceInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">IP address where a service or microservice instance is running</rdfs:comment>
        <rdfs:label xml:lang="en">IP</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/product#isDeleted -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/product#isDeleted">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Property used to determine whether a (business) product offering, a service or a microservice are currently running or available, or they have been deleted from the corresponding catalogues and/or infrastructure</rdfs:comment>
        <rdfs:label xml:lang="en">is deleted</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/devops-infra/product#port -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/devops-infra/product#port">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#MicroServiceInstance"/>
                    <rdf:Description rdf:about="http://w3id.org/devops-infra/product#ServiceInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Port where a service instance or microservice instance is running</rdfs:comment>
        <rdfs:label xml:lang="en">port</rdfs:label>
    </owl:DatatypeProperty>
    


