    <!-- http://w3id.org/devops-infra/product#BusinessProduct -->


    <owl:Class rdf:about="http://w3id.org/devops-infra/product#BusinessProduct">
        <rdfs:subClassOf rdf:resource="http://w3id.org/devops-infra/core#Resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#hasProductOffering"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/devops-infra/product#ProductOffering"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#offeredBy"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/devops-infra/organisation#Scope"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#uses"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/devops-infra/product#Service"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/devops-infra/product#isDeleted"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Business Product that is offered in the catalogue of an organisation (e.g., a content delivery network, an application, etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">Business Product</rdfs:label>
    </owl:Class>
    


